<html><head>
</head><body>
<div id="articles"></div>
<div id="contents">
<style type="text/css">
div#cover-<%= cover %> {
    color: <%= forecolor(bg) %>;
    background-color: <%= bg %>;
}
<% if (ubg) { %>
div#cover-<%= cover %>-u {
    color: <%= forecolor(ubg) %>;
    background-color: <%= ubg %>;
}
<% } %>
<% if (wbg) { %>
div#cover-<%= cover %>-w {
    color: <%= forecolor(wbg) %>;
    background-color: <%= wbg %>;
}
<% } %>
<% if (xbg) { %>
div#cover-<%= cover %>-x {
    color: <%= forecolor(xbg) %>;
    background-color: <%= xbg %>;
}
<% } %>
<% if (ybg) { %>
div#cover-<%= cover %>-y {
    color: <%= forecolor(ybg) %>;
    background-color: <%= ybg %>;
}
<% } %>
<% if (zbg) { %>
div#cover-<%= cover %>-z {
    color: <%= forecolor(zbg) %>;
    background-color: <%= zbg %>;
}
<% } %>
</style>
<div id="cover-north" class="cover">
<div id="cover-north-u" class="cover-u">维基日刊</div>
<div id="cover-north-w" class="cover-w"></div>
<div id="cover-north-x" class="cover-x"><ul></ul></div>
<div id="cover-north-y" class="cover-y"><ul></ul></div>
<div id="cover-north-z" class="cover-z"><ul></ul></div>
</div>
<div id="tocpage">
<div class="head-right">新闻</div>
<div class="toc-left"><ul id="toc-itn"></ul></div>
<div class="head-right">新知</div>
<div class="toc-left"><ul id="toc-dyk"></ul></div>
<div class="head-right">历史上的今天</div>
<div class="toc-left"><ul id="toc-otd"></ul></div>
<div class="head-right">特色</div>
<div class="toc-left"><ul id="toc-feature"></ul></div>
<div class="head-right">优良</div>
<div class="toc-left"><ul id="toc-good"></ul></div>
<div class="head-right">图像</div>
<div class="toc-left"><ul id="toc-featurepic"></ul></div>
</div>
</div>
</body></html>
