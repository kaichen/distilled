<%

var round = Math.round, ceil = Math.ceil;

%>

* {
    outline: 0;
    padding: 0;
    margin: 0;
    border: 0;
}

body {
    background-color: black;
    color: black;
    overflow: hidden;
}

#hposition {
    position:absolute;
    top:50%;
    left:50%;
    overflow:show;
}

#vposition {
    position:absolute;
    top:-<%= round(height/2) %>px;
    left:-<%= round(width/2) %>px;
}

